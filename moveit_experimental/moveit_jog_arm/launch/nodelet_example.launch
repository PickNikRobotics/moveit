<launch>
  <!-- Load URDF, SRDF -->
  <include file="$(find moveit_resources)/panda_moveit_config/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true"/>
  </include>

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <rosparam command="load" file="$(find moveit_jog_arm)/test/config/initial_position.yaml" />
    <param name="publish_frequency" type="double" value="50.0"/>
  </node>

  <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager" output="screen"/>

  <node pkg="nodelet" type="nodelet" name="jog_arm" args="load moveit/jog_arm nodelet_manager" output="screen">
    <param name="parameter_ns" type="string" value="jog_arm" />
    <rosparam command="load" file="$(find moveit_jog_arm)/test/config/jog_settings.yaml" />
  </node>

  <node pkg="nodelet" type="nodelet" name="commander" args="load moveit/jog_arm/nodelet_example nodelet_manager" output="screen"/>
</launch>
