<launch>
  <!-- The package has 2 examples of converting joystick commands to jog commands.
       This is the lower-latency C++ nodelet example. You can modify it in your own package for
       your own type of gamepad or joystick.
       It reduces latency (slightly) compared to spacenav_teleop_tools.launch.
       We don't plan to accept pull requests for other controller types using this method,
       because it would be too much to maintain.
       But, we do plan to accept teleop_tools config files (see spacenav_teleop_tools.launch)
  -->

  <node name="spacenav_node" pkg="spacenav_node" type="spacenav_node" />

  <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager" output="screen"/>

  <node pkg="nodelet" type="nodelet" name="jog_server" args="load moveit/jog_arm nodelet_manager" output="screen">
    <param name="parameter_ns" type="string" value="jog_server" />
    <rosparam command="load" file="$(find moveit_jog_arm)/config/ur_simulated_config.yaml" />
  </node>

  <node pkg="nodelet" type="nodelet" args="load moveit/jog_arm/spacenav_to_twist nodelet_manager" output="screen" />
</launch>
